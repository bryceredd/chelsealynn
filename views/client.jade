extends layout 

block content
  
  .client
    .download Click the image to download the HD version
    
  .allImages
    each image in images
      .contentImage
        a(href="/image/#{image}")
          img.centerImage( src="/fit/1600/1200/#{image}" )

  .menu
    .allMenu
      each image in images
        .menuImage 
          img( src="/fit/2000/200/#{image}" )
